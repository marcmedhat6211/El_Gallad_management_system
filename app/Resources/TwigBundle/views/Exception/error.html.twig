{% extends 'fe/base.html.twig' %}
{% block content %}
    <div class="container">
        <div class="row">
            <div class="col-lg-12 d-flex justify-content-center align-items-center">
                <div class="not-found-container">
                    <div class="not-found-text">
                        <p class="text">{{ status_code }}</p>
                    </div>
                    <p class="error-message text-center">{{ status_text }}</p>
                    <a href="{{ path("fe_home") }}" class="btn btn-style-3 teal-blue">{% trans %}Back To Homepage{% endtrans %}</a>
                </div>
            </div>
        </div>
    </div>
{% endblock %}